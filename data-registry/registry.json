      },
      "coverage": {
        "start_date": "1960-01-04",
        "end_date": "2024-12-31",
        "frequency": "daily"
      },
      "columns": ["date", "nyse_adv", "nyse_dec", "mcclellan_osc", "mcclellan_sum"],
      "column_indices": {
        "date": 0,
        "mcclellan_osc": 3,
        "mcclellan_sum": 4
      },
      "usage_notes": "CRITICAL: Use T-1 data (days_back=1) to avoid lookahead bias. Values range -200 to +200.",
      "validation_rules": {
        "expected_range": [-200, 200],
        "typical_values": "oscillates around 0"
      },
      "enables_components": ["IND-005", "IND-018"]
